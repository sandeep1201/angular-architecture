			from [cdc].[wwp_BarrierSection_CT] r with (nolock)
			where  (r.__$start_lsn <= @to_lsn)
			and (r.__$start_lsn >= @from_lsn)
			group by   r.[Id]) m
		on t.__$seqval = m.__$max_seqval_677FA233 and
		    ( (t.[Id] = m.[Id]) ) 	
