			when 1 then 1
			else 5
		end as __$operation,
		null as __$update_mask , t.[Id], t.[LegalSectionId], t.[ConvictionTypeID], t.[IsUnknown], t.[DateConvicted], t.[Details], t.[DeleteReasonId], t.[ModifiedBy], t.[ModifiedDate], t.[RowVersion]
