		(	select  r.[Id], max(r.__$seqval) as __$max_seqval_DE065F10
			from [cdc].[wwp_EmploymentInformation_CT] r with (nolock)
			where  (r.__$start_lsn <= @to_lsn)
			and (r.__$start_lsn >= @from_lsn)
