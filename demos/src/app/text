									   REGION_NUM           as REGION_NUM_S66             ,
									   ROW_NUMBER () OVER(ORDER BY PIN_NUM)  AS ID                                                             
  FROM  '+@DB2DBName+'.T0066_WP_IN_COMP                      
 WHERE  PIN_NUM          = '+@PIN_NUM+' 
   AND  HISTORY_CD       = 0     
