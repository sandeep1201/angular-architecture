	    case __$count_652A432A
	    when 1 then __$operation
	    else
			case __$min_op_652A432A 
				when 2 then 2
				when 4 then
				case __$operation
					when 1 then 1
					else 4
					end
				else
				case __$operation
					when 2 then 4
					when 4 then 4
					else 1
					end
			end
		end as __$operation,
		case __$count_652A432A
		when 1 then
