			and (t.__$start_lsn >= @from_lsn)
			and ((t.__$operation = 2) or (t.__$operation = 4) or 
				 ((t.__$operation = 1) and
				  (2 not in 
				 		(	select top 1 c.__$operation
							from [cdc].[wwp_BarrierSection_CT] c with (nolock)
							where  ( (c.[Id] = t.[Id]) )  
