			when 4 then __$update_mask
			else null
			end
		else	
			case __$min_op_652A432A 
			when 2 then null
			else
				case __$operation
				when 1 then null
				else __$update_mask 
				end
			end	
		end as __$update_mask , [Id], [ParticipantId], [ActionNeededPageId], [IsNoActionNeeded], [IsDeleted], [CreatedDate], [ModifiedDate], [ModifiedBy], [RowVersion]
	from
