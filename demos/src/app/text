	EXEC (@T0492Insert) AT NETWINFO_D00S_DB2

	END

	ELSE
	BEGIN



		SET @T0492Update = 'UPDATE ' + @DB2DBName + '.T0492_WP_DRIVER
						SET WPEN_REQR_IND = ''N'' ,
WPED_REQR_IND = ''N'' ,
