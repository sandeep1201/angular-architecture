-- =============================================
CREATE PROCEDURE [wwp].[USP_InsertLogEvent]
	@msg			VARCHAR(MAX),
	@level			TINYINT,
	@exception 		VARCHAR(MAX),
	@logEvent		VARCHAR(MAX)
AS

BEGIN
	SET NOCOUNT ON

	BEGIN TRY
		BEGIN TRAN;

		INSERT INTO wwp.LogEvent
		(
			[Message],
			MessageTemplate,
			[Level],
			[TimeStamp],
			Exception,
			Properties,
			LogEvent
		)
		VALUES
		(
			@msg,
			NULL,
			@level,
			GETDATE(),
			@exception,
			NULL,
			@logEvent
		);
		COMMIT TRAN;
